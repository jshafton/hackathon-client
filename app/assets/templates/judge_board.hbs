<div id="playerBoard">
  <div id="boardHeader">
    <div id="guessLabel">
        What google search generated this picture?
    </div>
  </div>
  <div id="guess" class="span6">
    <img id="searchImage" src="{{image_url}}" />
    <div id="timeLeftMessage">Time Left to Answer: 3 <span id="timeLeftSeconds"></span> Seconds</div>
  </div>
  <div id="players" class="span4">
    <h4>Player Answers</h4>
    <div class="well player-well">
      <form id="judgeAnswers">
        <div class="control-group">
          <ul id="answers" />
          <input type="button" class="btn btn-primary" value="Choose Answer" />
        </div>
      </form>
    </div>
  </div>
</div>
