<div id="playerBoard">
  <div id="boardHeader">
    <div id="guessLabel">
      What search generated this picture?
    </div>
  </div>
  <div id="guess" class="span6">
    <img id="searchImage" src="{{image_url}}" />
    <div id="guessArea"/>
    <div id="timeLeftMessage">Time Left to Answer: 3 <span id="timeLeftSeconds"></span> Seconds</div>
  </div>
  <div id="participants" class="span4">
    <h4>Judge</h4>
    <div id="judge" class="well player-well">
      <ul id="judgeList"/>
    </div>
    <h4>Players</h4>
    <div id="players" class="well player-well">
      <ul id="playerList"/>
    </div>
  </div>
</div>
